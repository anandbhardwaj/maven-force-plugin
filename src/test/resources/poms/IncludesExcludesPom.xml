<project>

  <parent>
    <groupId>com.force.test</groupId>
    <artifactId>ParentTestPom</artifactId>
    <version>1.0</version>
    <relativePath>./ParentTestPom.xml</relativePath>
  </parent>

  <modelVersion>4.0.0</modelVersion>
  <groupId>com.force.test</groupId>
  <artifactId>IncludesExcludes</artifactId>

  <build>
    <plugins>
      <plugin>
        <groupId>com.force</groupId>
        <artifactId>maven-force-plugin</artifactId>
        <configuration>
          <followReferences>true</followReferences>
          <includes>
            <include>Account</include>
          </includes>
          <excludes>
            <exclude>Contact</exclude>
          </excludes>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
